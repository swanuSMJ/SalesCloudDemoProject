<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Area wise Lead assignment</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>Pune_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.City</field>
                <operation>equals</operation>
                <value>Pune</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Mumbai_queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.City</field>
                <operation>equals</operation>
                <value>Mumbai</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Default_queue_lead_assignment</assignedTo>
            <assignedToType>Queue</assignedToType>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
